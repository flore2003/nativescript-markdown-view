{
  "name": "nativescript-markdown-view-angular",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts",
      "umdModuleIds": {
        "@nativescript/core": "ns-core",
        "@nativescript/angular": "ns-angular",
        "nativescript-markdown-view": "ns-ui-charts"
      }
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
